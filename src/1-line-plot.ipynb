   "source": [
    import matplotlib.pyplot as plt
import numpy as np

# Data
x = np.linspace(0, 10, 100)
y = x**2

# Create the plot
plt.plot(x, y)

# Customize the plot
plt.title("Plot of y = x^2")
plt.xlabel("x values")
plt.ylabel("y values (x^2)")
plt.grid(True)

# Display the plot
plt.show()
