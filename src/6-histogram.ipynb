    "import matplotlib.pyplot as plt\n",
    "data = [1, 2, 2, 3, 3, 3, 4, 4, 4, 4, 5]\n",
    "plt.hist(data, bins=5, color='skyblue', edgecolor='black')\n",
    "plt.title(\"Customized Histogram\")\n",
    "plt.xlabel(\"Values\")\n",
    "plt.ylabel(\"Frequency\")\n",
    "plt.show()"
